    <NMakeBuildCommandLine>setlocal enabledelayedexpansion

cd $(ProjectDir)

if exist lastSrcState.txt (
  call :writeSrcState newSrcState.txt

  fc lastSrcState.txt newSrcState.txt &gt; nul

  if ERRORLEVEL 1 (
    call :build
    call :writeSrcState lastSrcState.txt
  ) else (
    echo luaJIT is up to date.
  )

  del newSrcState.txt
) else (
  call :build
  call :writeSrcState lastSrcState.txt
)

goto:eof

:build
  call "$(DevEnvDir)..\Tools\VsDevCmd.bat" &amp; cd "$(ProjectDir)luajit\src\" &amp; msvcbuild.bat &amp; cd $(ProjectDir)
  echo done building
goto:eof

:writeSrcState
  if exist "%1" del "%1"
  set Line=
  for /f "delims=" %%a in ('dir /s luajit') do (
    if not "!Line!"=="" &gt;&gt;"%1" echo.!Line!
    set Line=%%a
  )
goto:eof</NMakeBuildCommandLine>
